{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/shop-back-end/src",
  "monorepo": true,
  "root": "apps/shop-back-end",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/shop-back-end/tsconfig.app.json"
  },
  "projects": {
    "shop-back-end": {
      "type": "application",
      "root": "apps/shop-back-end",
      "entryFile": "main",
      "sourceRoot": "apps/shop-back-end/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shop-back-end/tsconfig.app.json"
      }
    },
    "mp": {
      "type": "application",
      "root": "apps/mp",
      "entryFile": "main",
      "sourceRoot": "apps/mp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mp/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "db": {
      "type": "library",
      "root": "libs/db",
      "entryFile": "index",
      "sourceRoot": "libs/db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "blog": {
      "type": "application",
      "root": "apps/blog",
      "entryFile": "main",
      "sourceRoot": "apps/blog/src",
      "compilerOptions": {
        "tsConfigPath": "apps/blog/tsconfig.app.json"
      }
    }
  }
}